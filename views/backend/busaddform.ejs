
<%- include('../common/head.ejs')-%>
<%- include('../common/header.ejs')-%>
      <!-- partial -->
      <!-- partial:../../partials/_sidebar.html -->
      <%- include('../common/sidebar.ejs')-%>
      <!-- partial -->

<div class="main-panel">        
  <div class="content-wrapper">
    <div class="row">
      <div class="col-md-6 grid-margin stretch-card">
        <div class="card">
          <div class="card-body">
            <h4 class="card-title">Add New Bus </h4>
            <p class="card-description">
              <h10 style=" color: rgb(220, 20, 20);">
                <%- error %>
              </h10>
             
            </p>
            <form class="forms-sample" method="post" action="/bus">
              <div class="form-group">
                <label for="exampleInputUsername1">Bus Name</label>
                <input type="text" class="form-control"name ="busname" id="exampleInputUsername1" placeholder="Bus_Name">
              </div>
              <div class="form-group">
                <label for="exampleInputEmail1">Contact_No</label>
                <input type="text" class="form-control" name ="contact" id="exampleInputEmail1" placeholder="Contact_No">
              </div>
              <div class="form-group">
                <label for="exampleInputPassword1">Bus_Number</label>
                <input type="text" class="form-control" name ="busnumber" id="exampleInputPassword1" placeholder="Bus_Number">
              </div>
             
              
              <button type="submit" class="btn btn-primary me-2">Submit</button>
             
            </form>
            <button class="btn btn-light">Cancel</button>
          </div>
        </div>
      </div>
      
<%- include('../common/footer.ejs')-%>