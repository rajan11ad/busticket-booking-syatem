
<%- include('../common/head.ejs')-%>
<%- include('../common/header.ejs')-%>
      <!-- partial -->
      <!-- partial:../../partials/_sidebar.html -->
      <%- include('../common/sidebar.ejs')-%>
      <!-- partial -->
      
            <div class="col-lg-12 grid-margin stretch-card">
              <div class="card">
                <div class="card-body">
                  <h4 class="card-title">Customer Feedback Status</h4>
                  <p class="card-description">
                   <!-- <a href="/addnewbus" class="btn btn-success">Add Bus Route </a> --->
                  </p>
                  <div class="table-responsive">
                    <table class="table table-striped">
                      <thead>
                        <tr>
                          <th>
                            Id 
                          </th>
                          <th>
                           Name
                          </th>
                          <th>
                            Email
                          </th>
                          <th>
                            Message 
                          </th>
                         
                          <th>
                            Action
                          </th>
                        </tr>
                      </thead>
                      <tbody>
                        <% feedback.forEach((customer)=>{ %>
                           <tr>
                              <td><%-customer.id %></td>
                              <td><%-customer.name %></td>
                              <td><%-customer.email %></td>
                              <td> <%-customer.message %></td>
                              <td>
                                 <a class="btn btn-primary" href="/editcustomerdata/<%-customer.id %>">Views</a>                     
                                 <a class="btn btn-danger ml-2" href="/deletefeedback/<%-customer.id %>">Delete</a>   
                                
                             
                              </td>
                           </tr>
                           <%  }) %>
                      </tbody>
                    </table>
                  </div>
                </div>
              </div>
            </div>
            
        <!-- content-wrapper ends -->
        <!-- partial:../../partials/_footer.html -->
        <%- include('../common/footer.ejs')-%>


