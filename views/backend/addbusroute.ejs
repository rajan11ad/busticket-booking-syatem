<%- include('../common/head.ejs')-%>
<%- include('../common/header.ejs')-%>
      <!-- partial -->
      <!-- partial:../../partials/_sidebar.html -->
      <%- include('../common/sidebar.ejs')-%>
      <!-- partial -->

<div class="main-panel">        
  <div class="content-wrapper">
    <div class="row">
      <div class="col-md-6 grid-margin stretch-card">
        <div class="card">
          <div class="card-body">
            <h4 class="card-title">Add New Routes </h4>
            <p class="card-description">
              <h10 style=" color: rgb(220, 20, 20);">
                <%- routeerror %>
              </h10>
             
            </p>
            <form class="forms-sample"method="post" action="/addbusrouteform">
              <div class="form-group">
                <label for="exampleInputUsername1">Source</label>
                <input type="text" class="form-control"name ="source" id="exampleInputUsername1" placeholder="source">
              </div>
              <div class="form-group">
                <label for="exampleInputEmail1">Destination</label>
                <input type="text" class="form-control" name ="destination" id="exampleInputEmail1" placeholder="Destination">
              </div>
              <div class="form-group">
                <label for="exampleInputPassword1">Bus_Name</label>
                <input type="text" class="form-control" name ="bus_name" id="exampleInputPassword1" placeholder="Bus Name ">
              </div>
              <div class="form-group">
                <label for="exampleInputPassword1">Departure_date</label>
                <input type="date" class="form-control" name ="depature_date" min="2024-05-20" id="exampleInputPassword1" placeholder="Departure_Date">
              </div>
              <div class="form-group">
                <label for="exampleInputPassword1">Seat</label>
                <input type="text" class="form-control" name ="seat" id="exampleInputPassword1" placeholder="seat">
              </div>
              <div class="form-group">
                <label for="exampleInputPassword1">Time</label>
                <input type="time" class="form-control" name ="time" id="exampleInputPassword1" placeholder="Time">
              </div>
              <div class="form-group">
                <label for="exampleInputPassword1">bus_number</label>
                <input type="text" class="form-control" name ="busnumber" id="exampleInputPassword1" placeholder="Bus_number">
              </div>
              <div class="form-group">
                <label for="exampleInputPassword1">Cost</label>
                <input type="text" class="form-control" name ="cost" id="exampleInputPassword1" placeholder="Cost">
              </div>
              
             
              
              <button type="submit" class="btn btn-primary me-2">Submit</button>
             
            </form>
     
          </div>
        </div>
      </div>
      
<%- include('../common/footer.ejs')-%>